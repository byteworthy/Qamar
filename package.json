{
  "name": "my-app",
  "main": "client/index.js",
  "version": "1.0.0",
  "scripts": {
    "expo:dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN EXPO_PUBLIC_DOMAIN=$REPLIT_DEV_DOMAIN:5000 npx expo start --localhost",
    "server:dev": "tsx server/index.ts",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "db:push": "drizzle-kit push",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "typecheck": "npm run check:types",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\"",
    "test": "npm run test:server",
    "test:server": "jest --config jest.config.js",
    "test:client": "jest --config jest.config.mobile.js",
    "test:watch": "jest --watch",
    "verify": "npm run check:types && npm test",
    "verify:local": "npm run check:types && npm test",
    "ship": "node tools/ship.mjs",
    "release:check": "npm run check:types && npm run lint && npm test",
    "eas:build:dev": "eas build --profile development",
    "eas:build:preview": "eas build --profile preview",
    "eas:build:prod": "eas build --profile production",
    "eas:submit:prod": "eas submit --profile production",
    "eas:submit:ios": "eas submit --profile production --platform ios",
    "eas:submit:android": "eas submit --profile production --platform android"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@sentry/node": "^10.35.0",
    "@tanstack/react-query": "^5.90.7",
    "@types/cookie-parser": "^1.4.10",
    "cookie-parser": "^1.4.7",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.1",
    "expo": "^54.0.23",
    "expo-blur": "^15.0.7",
    "expo-constants": "~18.0.9",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.9",
    "expo-glass-effect": "~0.1.6",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.8",
    "expo-notifications": "^0.32.16",
    "expo-server-sdk": "^4.0.0",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "http-proxy-middleware": "^3.0.5",
    "p-limit": "^7.2.0",
    "p-retry": "^7.1.1",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-iap": "^14.7.6",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-share": "^12.2.4",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "stripe": "^20.0.0",
    "stripe-replit-sync": "^1.0.0",
    "tsx": "^4.20.6",
    "ws": "^8.18.0",
    "zod": "^3.25.76",
    "zod-validation-error": "^3.5.4"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "24.10.0",
    "@types/pg": "^8.16.0",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.16",
    "prettier": "3.6.2",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
